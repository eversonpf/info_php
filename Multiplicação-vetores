$a = [2, 3, 4];
    $b = [5, 6, 7];

    $resultado = [];

    /*$result0 = $a[0] * $b[0];
    array_push($resultado, $result0);

    $result1 = $a[1] * $b[1];
    array_push($resultado, $result1);

    $result2 = $a[2] * $b[2];
    array_push($resultado, $result2);
  */
   for ($i = 0; $i < count($a); $i++ ){
       $result = $a [$i] * $b [$i];
       array_push($resultado,$result);

   }   

   $mostraResultado = implode(",", $resultado);

   echo "o produto Ã© :{$mostraResultado}";
